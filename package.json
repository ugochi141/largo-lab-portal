{
  "name": "largo-lab-portal",
  "version": "2.1.0",
  "description": "Kaiser Permanente Largo Laboratory Operations Portal - Enterprise Edition with Manager Operations Suite",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "audit": "npm audit --audit-level=moderate",
    "build": "npm run lint && npm test",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop all",
    "pm2:restart": "pm2 restart all",
    "pm2:logs": "pm2 logs",
    "backup": "node scripts/backup.js",
    "health-check": "curl http://localhost:3000/health"
  },
  "keywords": [
    "healthcare",
    "laboratory",
    "HIPAA",
    "CLIA",
    "CAP",
    "Kaiser Permanente",
    "medical"
  ],
  "author": "Kaiser Permanente Largo Laboratory",
  "license": "PROPRIETARY",
  "dependencies": {
    "@sentry/node": "^7.73.0",
    "@sentry/tracing": "^7.73.0",
    "axios": "^1.5.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.10.0",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "joi": "^17.10.2",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.10.1",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "uuid": "^9.0.1",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1",
    "xlsx": "^0.18.5",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@types/express": "^4.17.19",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.6",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaiserpermanente/largo-lab-portal"
  }
}
